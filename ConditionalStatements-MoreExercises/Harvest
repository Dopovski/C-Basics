#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double x;  
    cin >> x;
    double y;  
    cin >> y;
    double z;  
    cin >> z;
    double workers;
    cin >> workers;

    double totalGrapes = x * y;
    double wine = (totalGrapes * 0.40) / 2.5;  

    cout.setf(ios::fixed);
    cout.precision(0);

    if (wine >= z) {
        double leftWine = wine - z;
        double winePerPerson = leftWine / workers;

        cout << "Good harvest this year! Total wine: " 
             << floor(wine) << " liters." << endl;
        cout << leftWine << " liters left -> " 
             << ceil(winePerPerson) << " liters per person." << endl;
    } else {
        double neededWine = z - wine;
        cout << "It will be a tough winter! More " 
             << floor(neededWine) << " liters wine needed." << endl;
    }

    return 0;
}

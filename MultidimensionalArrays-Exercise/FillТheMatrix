#include <iostream>
#include <vector>
#include <sstream>

using namespace std;

vector<vector<int>> methodA(int n) {
    vector<vector<int>> mat(n, vector<int>(n, 0));
    int counter = 1;
    for (int j = 0; j < n; j++) {
        for (int i = 0; i < n; i++) {
            mat[i][j] = counter++;
        }
    }
    return mat;
}

vector<vector<int>> methodB(int n) {
    vector<vector<int>> mat(n, vector<int>(n, 0));
    int counter = 1;
    for (int j = 0; j < n; j++) {
        int srow, erow, irow;
        if (j % 2) {
            srow = n - 1; 
            erow = 0; 
            irow = -1;
        }
        else {
            srow = 0; 
            erow = n - 1; 
            irow = 1;
        }
        for (int i = srow;; i += irow) {
            mat[i][j] = counter++;
            if (i == erow) {
                break;
            }
        }
    }
    return mat;
}

int main() {
    int n;
    char m;

    string nstr;
    getline(cin, nstr, ',');
    n = stoi(nstr);
    cin >> m;

    vector<vector<int>> mat = (m == 'A' ? methodA(n) : methodB(n));

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cout << mat[i][j] << " ";
        }
        cout << endl;
    }
}

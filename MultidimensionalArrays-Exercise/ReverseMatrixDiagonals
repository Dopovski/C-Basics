#include <iostream>
#include <vector>
using namespace std;

int main() {
    int R, C;
    cin >> R >> C;

    vector<vector<int>> mat(R, vector<int>(C));
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            cin >> mat[i][j];
        }
    }

    for (int sum = R + C - 2; sum >= 0; sum--) {
        vector<int> diag;

        for (int i = 0; i < R; i++) {
            int j = sum - i;
            if (j >= 0 && j < C) {
                diag.push_back(mat[i][j]);
            }
        }
        
        for (int i = diag.size() - 1; i >= 0; i--) {
            cout << diag[i] << " ";
        }
        cout << endl;
    }

    return 0;
}

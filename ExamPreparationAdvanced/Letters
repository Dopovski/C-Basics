#include <iostream>
#include <set>
#include <sstream>
#include <algorithm>

using namespace std;

int main() {
    string line;
    getline(cin, line);
    
    for (char &c : line) {
        if (!isalpha(c)) {
            c = ' ';
        }
    }

    istringstream iss(line);
    set<string> words;
    string word;

    while (iss >> word) {
        words.insert(word);
    }

    while (true) {
        getline(cin, line);

        if (line == ".") {
            break;
        }

        char symbol = tolower(line[0]);
        bool found = false;

        for (const string &w : words) {
            string temp = w;
            transform(temp.begin(), temp.end(), temp.begin(), ::tolower);

            if (temp.find(symbol) != string::npos) {
                cout << w << " ";
                found = true;
            }
        }

        if (!found) {
            cout << "---";
        }

        cout << endl;
    }

    return 0;
}

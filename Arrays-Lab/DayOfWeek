#include <iostream>

using namespace std;

string numToDay (int weekDay) {
    static string weekDayName[] = {
        "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"
    };
    if (weekDay < 1 || weekDay > 7) {
        return "Invalid day!";
    }
    return weekDayName[weekDay-1];
}

int main() {
    int num;
    cin >> num;

    cout << numToDay(num) << endl;
}

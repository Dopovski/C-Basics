#include <iostream>

using namespace std;

const int MAX_NUMBERS_SIZE = 99;

int main() {
    int num [MAX_NUMBERS_SIZE];
    int n;
    cin >> n;

    for (int i = 0; i < n; i++) {
        cin >> num[i];
    }
    for (int i = n - 1; i >= 0; i--) {
        cout << num[i] << ' ';
    }
    cout << endl;
}

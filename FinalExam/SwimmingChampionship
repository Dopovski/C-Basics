#include <iostream>
#include <vector>
#include <iomanip>

using namespace std;

double Expenses(int days, int swimmers, double hotelPrice, double participationFee) {
    double hotelCost = days * swimmers * hotelPrice;
    double participationCost = swimmers * participationFee;
    return hotelCost + participationCost;
}

double Points(const vector<double>& points) {
    double totalPoints = 0.0;

    for (size_t i = 0; i < points.size(); i++) {
        totalPoints += points[i];

        if (i > 0) {
            totalPoints += points[i - 1] * 0.05;
        }
    }

    return totalPoints;
}

double MoneyLeft(double totalExpenses, bool success) {
    double percentToPay = success ? 0.75 : 0.90;
    return totalExpenses * percentToPay;
}

int main() {
    int days, swimmers;
    double neededPoints, hotelPrice, participationFee;

    cin >> days >> neededPoints >> swimmers >> hotelPrice >> participationFee;

    vector<double> dailyPoints(days);
    for (int i = 0; i < days; i++) {
        cin >> dailyPoints[i];
    }

    double totalExpenses = Expenses(days, swimmers, hotelPrice, participationFee);
    double totalPoints = Points(dailyPoints);

    bool success = totalPoints >= neededPoints;
    double moneyLeft = MoneyLeft(totalExpenses, success);

    cout << fixed << setprecision(2);
    cout << "Money left to pay: " << moneyLeft << " BGN." << endl;

    if (success) {
        cout << "The championship was successful!" << endl;
    } else {
        cout << "The championship was not successful." << endl;
    }

    return 0;
}

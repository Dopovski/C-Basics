#include <iostream>
#include <sstream>
#include <set>
#include <algorithm>
#include <vector>

using namespace std;

int main() {
    string line;
    getline(cin, line); 

    stringstream ss(line);
    string word;
    set<string> shortWordsSet; 

    while (ss >> word) {
        for (char &c : word) c = tolower(c);
        if (word.length() < 5) {
            shortWordsSet.insert(word);
        }
    }
    
    vector<string> shortWords(shortWordsSet.begin(), shortWordsSet.end());
    sort(shortWords.begin(), shortWords.end());
    
    for (size_t i = 0; i < shortWords.size(); i++) {
        if (i > 0) cout << ", ";
        cout << shortWords[i];
    }
    cout << endl;

    return 0;
}

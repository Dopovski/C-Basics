#include <iostream>
#include <vector>
#include <sstream>
#include <algorithm>

using namespace std;

int main() {
    string line;
    getline(cin, line); 

    stringstream ss(line);
    double num;
    vector<double> nums;
    
    while (ss >> num) {
        nums.push_back(num);
    }
    
    sort(nums.begin(), nums.end(), greater<double>());
    
    int limit = min(3, (int)nums.size());
    for (int i = 0; i < limit; i++) {
        if (i > 0) cout << " "; 
        cout << nums[i];
    }
    cout << endl;

    return 0;
}

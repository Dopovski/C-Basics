#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    vector<double> nums;
    double x;

    while (cin >> x) {
        nums.push_back(x);
    }

    if (nums.empty()) {
        return 0;
    }

    sort(nums.begin(), nums.end());

    int count = 1;
    for (size_t i = 1; i < nums.size(); i++) {
        if (nums[i] == nums[i - 1]) {
            count++;
        } else {
            cout << nums[i - 1] << " -> " << count << endl;
            count = 1;
        }
    }

    cout << nums.back() << " -> " << count << endl;

    return 0;
}

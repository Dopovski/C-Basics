#include <iostream>
#include <algorithm>

using namespace std;

int calculate(const string & s1, const string & s2) {
    int res = 0;
    size_t a = s1.length();
    size_t b = s2.length();
    size_t last = max(a, b);

    for (size_t i = 0; i < last; i++) {
        if (i < a && i < b) {
            res += s1[i] * s2[i];
        }
        else if (i < a) {
            res += s1[i];
        }
        else {
            res += s2[i];
        }
    }

    return res;
}

int main() {
    string first, second;
    cin >> first >> second;

    int sum = calculate(first, second);
    cout << sum << endl;
    return 0;
}

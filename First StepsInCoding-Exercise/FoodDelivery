#include <iostream>

using namespace std;

int main() {
    int chicken,fish, vegetarian;
    cin >> chicken >> fish >> vegetarian;

    double Price = chicken * 10.35 + fish * 12.40 + vegetarian * 8.15;
    double dessertPrice = Price * 0.2;

    double totalPrice = Price + dessertPrice + 2.50;
    cout << totalPrice;
}

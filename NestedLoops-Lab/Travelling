#include <iostream>
#include <string>
using namespace std;
int main()
{
    string destination;

    while (cin >> destination && destination != "End") {
        int money;
        cin >> money;

        double savedMoney = 0;
        while (savedMoney < money) {
            double amount;
            cin >> amount;
            savedMoney += amount;
        }
        cout << "Going to " << destination << "!" << endl;
    }
    return 0;
}

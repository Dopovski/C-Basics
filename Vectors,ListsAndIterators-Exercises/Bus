#include <iostream>
#include <vector>
#include <climits>

using namespace std;

int timeInMinutes(int hhmm) {
    return (hhmm / 100) * 60 + (hhmm % 100);
}

int main() {
    int N;
    cin >> N;

    vector<int> buses(N);
    for (int i = 0; i < N; i++) {
        cin >> buses[i];
    }

    int trainTime;
    cin >> trainTime;

    int trainMinutes = timeInMinutes(trainTime);

    int minWait = INT_MAX;
    int bestBusIndex = -1;

    for (int i = 0; i < N; i++) {
        int busMinutes = timeInMinutes(buses[i]);
        if (busMinutes <= trainMinutes) {
            int wait = trainMinutes - busMinutes;
            if (wait < minWait) {
                minWait = wait;
                bestBusIndex = i + 1;
            }
        }
    }

    cout << bestBusIndex << endl;

    return 0;
}

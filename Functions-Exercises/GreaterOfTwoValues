#include <iostream>
#include <string>
using namespace std;

int getMaxInt(int a, int b) {
    return (a > b) ? a : b;
}

char getMaxChar(char a, char b) {
    return (a > b) ? a : b;
}

string getMaxString(const string& a, const string& b) {
    return (a > b) ? a : b;
}

int main() {
    string type;
    getline(cin, type);

    string first, second;
    
    getline(cin, first);
    getline(cin, second);

    if (type == "int") {
        int a = stoi(first);
        int b = stoi(second);
        cout << getMaxInt(a, b);
    }
    else if (type == "char") {
        char a = first[0];
        char b = second[0];
        cout << getMaxChar(a, b);
    }
    else if (type == "string") {
        cout << getMaxString(first, second);
    }

    return 0;
}

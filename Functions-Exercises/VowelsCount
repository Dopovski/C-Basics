#include <iostream>

using namespace std;

bool isVowel(char ch) {
    switch (ch) {
        case 'a': case 'e': case 'i': case 'o': case 'u': case 'y':
            case 'A': case 'E': case 'I': case 'O': case 'U': case 'Y': return true;
    }
    return false;
}

int countVowels(string word) {
    int vowelsCount = 0;
    for (int i = 0; i < word.length(); i++) {
        if (isVowel (word[i])) {
            vowelsCount++;
        }
    }
    return vowelsCount;
}

int main() {
    string word;
    cin >> word;

    cout << countVowels(word);
}

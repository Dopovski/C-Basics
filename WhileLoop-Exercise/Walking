#include <iostream>
#include <string>
using namespace std;
int main() {
    int steps = 0;
    int target = 10000;

    bool isHome = false;

    while (steps < target && !isHome) {
        string input;
        getline(cin, input);

        int currentSteps;

        if (input == "Going home") {
            cin >> currentSteps;
            isHome = true;
        }
        else {
            currentSteps = stoi(input);
        }
        steps += currentSteps;
    }

    if (steps >= target) {
        cout << "Goal reached! Good job!" << endl;
    }
    else {
        cout << target - steps << " more steps to reach goal." << endl;
    }
}

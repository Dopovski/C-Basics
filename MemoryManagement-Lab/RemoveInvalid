#include <iostream>
#include <sstream>
#include <string>

using namespace std;

int main() {
    string line;

    int capacity = 10;
    int count = 0;

    int* ids = new int[capacity];
    string* names = new string[capacity];

    while (true) {
        getline(cin, line);

        if (line == "end") {
            break;
        }

        stringstream ss(line);
        int id;
        string name;

        ss >> id >> name;

        if (id >= 0) {
            if (count == capacity) {
                capacity *= 2;

                int* newIds = new int[capacity];
                string* newNames = new string[capacity];

                for (int i = 0; i < count; i++) {
                    newIds[i] = ids[i];
                    newNames[i] = names[i];
                }

                delete[] ids;
                delete[] names;

                ids = newIds;
                names = newNames;
            }

            ids[count] = id;
            names[count] = name;
            count++;
        }
    }

    for (int i = 0; i < count; i++) {
        cout << ids[i] << " " << names[i] << endl;
    }

    delete[] ids;
    delete[] names;

    return 0;
}

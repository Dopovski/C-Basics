#include <iostream>
#include <cmath>
using namespace std;
int main() {
    double a, b, c;
    cin >> a >> b >> c;
    double disc = b * b - 4 * a * c;

    if (disc < 0) {
        cout << "no roots";
    }
    else {
        disc = sqrt(disc);
        cout << (-b + disc) / (2 * a) << " ";
    }
    if (disc > 0) {
        cout << (-b - disc) / (2 * a);

    }
}

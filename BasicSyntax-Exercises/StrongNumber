#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    int sum = 0;
    int temp = n;

    while (temp != 0) {
        int digit = temp % 10;

        int fact = 1;
        for (int i = 2; i <= digit; i++) {
            fact *= i;
        }

        sum += fact;  
        temp /= 10;
    }

    cout << (sum == n ? "yes" : "no");
    return 0;
}

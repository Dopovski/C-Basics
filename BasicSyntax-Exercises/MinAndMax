#include <iostream>
using namespace std;
int main() {
    int n;
    cin >> n;

    int min, max;

    for (int i = 0; i < n; i++) {
        int num;
        cin >> num;
        if (i == 0) {
            min = num;
            max = num;
        }
        else {
            if (num < min) min = num;
            if (num > max) max = num;
        }
    }
    cout << min << " " << max;
}

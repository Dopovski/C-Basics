#include <iostream>
using namespace std;

int main() {
    int a, b;
    cin >> a >> b;
    
    if (a > b) swap(a, b);

    int gcd = 1;

    for (int i = a; i >= 1; i--) {
        if (a % i == 0 && b % i == 0) {
            gcd = i;
            break;
        }
    }

    cout << gcd << endl;
    return 0;
}

#include <iostream>
#include <stack>
#include <vector>

using namespace std;

int main() {
    int N;
    cin >> N;

    stack<int> s;
    stack<int> maxS;
    stack<int> minS;

    for (int i = 0; i < N; i++) {
        int type;
        cin >> type;

        if (type == 1) {
            int x;
            cin >> x;
            s.push(x);

            if (maxS.empty() || x >= maxS.top())
                maxS.push(x);

            if (minS.empty() || x <= minS.top())
                minS.push(x);
        }
        else if (type == 2) {
            if (!s.empty()) {
                int top = s.top();
                s.pop();

                if (!maxS.empty() && top == maxS.top())
                    maxS.pop();

                if (!minS.empty() && top == minS.top())
                    minS.pop();
            }
        }
        else if (type == 3) {
            if (!maxS.empty())
                cout << maxS.top() << endl;
        }
        else if (type == 4) {
            if (!minS.empty())
                cout << minS.top() << endl;
        }
    }

    vector<int> result;
    while (!s.empty()) {
        result.push_back(s.top());
        s.pop();
    }

    for (size_t i = 0; i < result.size(); i++) {
        cout << result[i];
        if (i != result.size() - 1)
            cout << ", ";
    }

    return 0;
}

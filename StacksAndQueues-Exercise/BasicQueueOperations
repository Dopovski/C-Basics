#include <iostream>
#include <queue>
#include <climits>

using namespace std;

int main() {
    int N, S, X;
    cin >> N >> S >> X;

    queue<int> q;
    
    for (int i = 0; i < N; i++) {
        int value;
        cin >> value;
        q.push(value);
    }
    
    for (int i = 0; i < S && !q.empty(); i++) {
        q.pop();
    }
    
    if (q.empty()) {
        cout << 0 << endl;
        return 0;
    }

    bool found = false;
    int minElement = INT_MAX;
    
    while (!q.empty()) {
        int current = q.front();
        q.pop();

        if (current == X) {
            found = true;
        }

        if (current < minElement) {
            minElement = current;
        }
    }
    
    if (found) {
        cout << "true" << endl;
    } else {
        cout << minElement << endl;
    }

    return 0;
}

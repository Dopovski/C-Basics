#include <iostream>
#include <map>
#include <vector>

using namespace std;

using FrequencyMap = map<int, int>;

void solve() {
    int n;
    if (!(cin >> n)) return;

    FrequencyMap counts;
    
    for (int i = 0; i < n; ++i) {
        int currentNumber;
        cin >> currentNumber;
        
        counts[currentNumber]++;
    }
    
    for (auto const& [number, count] : counts) {
        if (count % 2 == 0) {
            cout << number << endl;
            return;
        }
    }
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

    solve();

    return 0;
}

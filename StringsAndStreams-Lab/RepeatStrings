#include <iostream>
#include <sstream>

using namespace std;

int main(void) {
    string buffer;
    getline(cin, buffer);

    istringstream iss(buffer);
    string word;

    while (iss >> word) {
        for (int i =0; i < word.length(); i++) {
            cout << word;
        }
    }
    cout << endl;
}

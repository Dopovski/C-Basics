#include <iostream>

using namespace std;
const int MAX_SIZE_ARRAY = 100;

void readArray(int array[], int size) {
    for (int i = 0; i < size; i++) {
        cin >> array[i];
    }
}

int main() {
    int numbers[MAX_SIZE_ARRAY] = {};
    int n;
    cin >> n;
    readArray(numbers, n);

    int magic;
    cin >> magic;

    for (int i = 0; i < n - 1 ; i++) {
        for (int j = i + 1; j < n; j++) {
            if (numbers[i] + numbers[j] == magic) {
                cout << numbers[i] << " " << numbers[j] << endl;
            }
        }
    }
}

#include <iostream>
using namespace std;

const int MAX = 100;

int main() {
    int rows, cols;
    int matrix[MAX][MAX];

    cin >> rows >> cols;

    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            cin >> matrix[i][j];
        }
    }

    for (int j = 0; j < cols; j++) {
        int sum = 0;
        for (int i = 0; i < rows; i++) {
            sum += matrix[i][j];
        }
        cout << sum << endl;
    }

    return 0;
}

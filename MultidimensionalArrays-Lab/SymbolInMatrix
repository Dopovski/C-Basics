#include <iostream>
using namespace std;

const int MAX = 100;

int main() {
    int N;
    char matrix[MAX][MAX];

    cin >> N;

    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            cin >> matrix[i][j];
        }
    }

    char symbol;
    cin >> symbol;

    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            if (matrix[i][j] == symbol) {
                cout << "(" << i << ", " << j << ")";
                return 0;
            }
        }
    }
    
    cout << symbol << " does not occur in the matrix";

    return 0;
}

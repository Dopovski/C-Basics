#include <iostream>
using namespace std;

const int MAX = 100;

int main() {
    int N;
    int matrix[MAX][MAX];

    cin >> N;

    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            cin >> matrix[i][j];
        }
    }

    int sum = 0;
    for (int i = 0; i < N; i++) {
        sum += matrix[i][i];
    }

    cout << sum;

    return 0;
}

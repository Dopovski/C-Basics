#include <iostream>
using namespace std;
int main()
{
    int n;
    cin >> n;
    int curent = 1;
    bool isBigger = false;
    for (int i = 1; i <= n; i++)
    {
        for (int j = 1; j <= i; j++) {
            if (curent > n) {
                isBigger = true;
                break;
            }
            cout << curent << " ";
            curent++;
        }
        if (isBigger) {
            break;
        }
        cout << endl;
    }
}

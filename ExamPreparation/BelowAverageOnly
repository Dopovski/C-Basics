#include <iostream>
#include <vector>

using namespace std;

vector<int> filter(const vector<int>& arr) {
    int totalSum = 0;
    for (int x : arr) {
        totalSum += x;
    }

    double average = static_cast<double>(totalSum) / arr.size();

    vector<int> filtered;
    for (int x : arr) {
        if (x <= average) {
            filtered.push_back(x);
        }
    }

    return filtered;
}

int main() {
    int n;
    cin >> n;

    vector<int> arr(n);

    int sum = 0;
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
        sum += arr[i];
    }
    double average = static_cast<double>(sum) / n;
    vector<int> filtered = filter(arr);

    int sumEven = 0;
    int sumOdd = 0;

    for (int i = 0; i < filtered.size(); i++) {
        if (i % 2 == 0) {
            sumEven += filtered[i];
        } else {
            sumOdd += filtered[i];
        }
    }

    int result = sumEven * sumOdd;
    cout << result << endl;

    return 0;
}

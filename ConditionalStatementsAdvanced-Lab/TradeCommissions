#include <iostream>
#include<string>

using namespace std;

int main()
{
    string cityName;
    cin >> cityName;

    double amountProfit;
    cin >> amountProfit;

    cout.setf(ios::fixed);
    cout.precision(2);

    if (cityName == "Sofia") 
    {
        if (amountProfit >= 0 && amountProfit <= 500) 
        {
            cout << amountProfit * 0.05;
        }
        else if (amountProfit > 500 && amountProfit <= 1000) 
        {
            cout << amountProfit * 0.07;
        }
        else if (amountProfit > 1000 && amountProfit <= 10000) 
        {
            cout << amountProfit * 0.08;
        }
        else if (amountProfit > 10000) 
        {
            cout << amountProfit * 0.12;
        }
        else 
        {
            cout << "error";
        }
    }
    else if (cityName == "Varna") {
        if (amountProfit >= 0 && amountProfit <= 500) 
        {
            cout << amountProfit * 0.045;
        }
        else if (amountProfit > 500 && amountProfit <= 1000) 
        {
            cout << amountProfit * 0.075;
        }
        else if (amountProfit > 1000 && amountProfit <= 10000) 
        {
            cout << amountProfit * 0.1;
        }
        else if (amountProfit > 10000) 
        {
            cout << amountProfit * 0.13;
        }
        else 
        {
            cout << "error";
        }
    }
    else if (cityName == "Plovdiv") {
        if (amountProfit >= 0 && amountProfit <= 500) 
        {
            cout << amountProfit * 0.055;
        }
        else if (amountProfit > 500 && amountProfit <= 1000) 
        {
            cout << amountProfit * 0.08;
        }
        else if (amountProfit > 1000 && amountProfit <= 10000) 
        {
            cout << amountProfit * 0.12;
        }
        else if (amountProfit > 10000) 
        {
            cout << amountProfit * 0.145;
        }
        else 
        {
            cout << "error";
        }
    }
    else 
    {
        cout << "error";
    }

    return 0;
}

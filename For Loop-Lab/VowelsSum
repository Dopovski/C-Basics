#include <iostream>
#include <string>

using namespace std;

int main() {
    string s;
    getline(cin, s);
    int sum = 0;
    for (int i = 0; i <= s.length() - 1; i++) {
      char Letter = s[i];
        if (Letter == 'a') {
            sum += 1;
        }
        else if (Letter == 'e') {
            sum += 2;
        }
        else if (Letter == 'i') {
            sum += 3;
        }
        else if (Letter == 'o') {
            sum += 4;
        }
        else if (Letter == 'u') {
            sum += 5;
        }
    }
    cout << sum << endl;
}

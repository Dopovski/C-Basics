#include <iostream>
#include <stack>
#include <string>

using namespace std;

int main() {
    string expression;
    getline(cin, expression);

    stack<int> brackets;

    for (int i = 0; i < expression.length(); i++) {
        if (expression[i] == '(') {
            brackets.push(i);
        }
        else if (expression[i] == ')') {
            int start = brackets.top();
            brackets.pop();

            cout << expression.substr(start, i - start + 1) << endl;
        }
    }

    return 0;
}

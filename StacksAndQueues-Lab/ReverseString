#include <iostream>
#include <string>
#include <sstream>
#include <stack>

using namespace std;

int main() {
    string line;
    getline(cin, line);

    stack<char> st;
    for (char c : line) {
        st.push(c);
    }
    while (!st.empty()) {
        cout << st.top();
        st.pop();
    }
}

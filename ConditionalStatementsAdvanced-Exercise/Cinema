#include <iostream>
#include <string>

using namespace std;

int main() {
    string projection;
    cin >> projection;

    int rows;
    cin >> rows;

    int columns;
    cin >> columns;

    double price = 0;

    if (projection == "Premiere") {
        price = 12;
    }
    else if (projection == "Normal") {
        price = 7.5;
    }
    else if (projection == "Discount") {
        price = 5;
    }

    int allSeats = rows * columns;
    double totalPrice = allSeats * price;

    cout.setf(ios::fixed);
    cout.precision(2);
    cout << totalPrice << " leva" << endl;
}

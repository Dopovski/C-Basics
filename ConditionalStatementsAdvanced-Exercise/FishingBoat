#include <iostream>

using namespace std;

int main() {
    int budget;
    cin >> budget;

    string season;
    cin >> season;

    int fishermen;
    cin >> fishermen;

    double priceShip = 0;

    if (season == "Spring") {
        priceShip = 3000;
    }
    else if (season == "Summer" || season == "Autumn") {
        priceShip = 4200;
    }
    else if (season == "Winter") {
        priceShip = 2600;
    }
    if (fishermen <= 6) {
        priceShip -= priceShip * 0.10;
    }
    else if (fishermen >= 7 && fishermen <= 11) {
        priceShip -= priceShip * 0.15;
    }
    else if (fishermen >= 12) {
        priceShip -= priceShip * 0.25;
    }
    if (fishermen % 2 == 0 && season != "Autumn") {
        priceShip -= priceShip * 0.05;
    }
    cout.setf(ios::fixed);
    cout.precision(2);
    if (budget >= priceShip) {
        cout << "Yes! You have " << budget - priceShip << " leva left." << endl;
    }
    else {
        cout << "Not enough money! You need " << priceShip - budget << " leva." << endl;
    }

}
